{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "azureStorageLocation": {
      "type": "String"
    },
    "azureStorageName": {
      "type": "String"
    },
    "azureStorageType": {
      "type": "String"
    },
    "existingStorageRG": {
      "type": "string"
    },
    "informaticaTags": {
      "type": "object"
    }
  },
  "variables": {
  },
  "resources": [
  ],
  "outputs": {
    "storageId": {
      "value": "[resourceId(parameters('existingStorageRG'), 'Microsoft.Storage/storageAccounts', parameters('azureStorageName'))]",
      "type": "string"
    },
    "storageRef": {
      "value": "[reference(resourceId(parameters('existingStorageRG'), 'Microsoft.Storage/storageAccounts', parameters('azureStorageName'))).primaryEndpoints.blob]",
      "type": "string"
    }
  }
}